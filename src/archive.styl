article ul
  list-style-type none
  padding-left 60px

  a
    text-decoration none
    color #4183c4

    &:hover
      text-decoration underline

  li
    font-size 20px

.paging
  position relative
  min-height 70px

  a
    text-decoration none
    color #4183c4
    font-weight bold

    &:hover
      text-decoration none

  .paging-next, .paging-prev
    position absolute
    font-size 20px
    padding 3px


  .paging-next
    right 0
    padding-left 5px

  .paging-prev
    padding-right 5px
